                "stages": [{
                        "actions": [{
                                "actionTypeId": {
                                    "category": "Source",
                                    "owner": "ThirdParty",
                                    "provider": "GitHub",
                                    "version": "1"
                                },
                                "configuration": {
                                    "Branch": "develop",
                                    "OAuthToken": "{{resolve:secretsmanager:GitHubAuth:SecretString:token}}",
                                    "Owner": "Rashair",
                                    "PollForSourceChanges": "false",
                                    "Repo": "HRPotter-2.0"
                                },
                                "inputArtifacts": [],
                                "name": "Source",
                                "namespace": "SourceVariables",
                                "outputArtifacts": [{
